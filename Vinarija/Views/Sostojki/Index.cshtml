@{
    ViewBag.Title = "SostojkiTitle";
}

<html>
<head>
    <title>
        Sostojki
    </title>
    
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

</head>
<body>
    <a href="/Home/Index" class="btn btn-default">Home</a>
    <table id="ecample">

        <tr>
            <th>Tip Vino</th>
            <th>Seker</th>
            <th>Grozje</th>
            <th>Voda</th>
            <th>Ostanato</th>
        </tr>

        @foreach (var obj in ViewData.Model)
        {
            <tr>
                <td>

                    <ul><li>@obj.Vino.ImeVino</li></ul>
                </td>
                <td>
                    <ul><li>@obj.Seker</li></ul>
                </td>
                <td>

                    <ul><li>@obj.Grozje</li></ul>
                </td>
                <td>
                    <ul><li>@obj.Voda</li></ul>
                </td>
                <td>
                    <ul><li>@obj.Ostanato</li></ul>
                </td>
            </tr>
        }

    </table>
    <button>Nesto</button>
    <script>
        $(document).ready(function () {
            $("button").click(function () {
                $("button").hide();
            });
        });
    </script>
</body>
</html>

